#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Volum %d%% (heshtuar)</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:356
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Volum %d%%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:521
msgid "Output"
msgstr "Dalje"

#: ../panel-plugin/pulseaudio-menu.c:561
msgid "Input"
msgstr "Hyrje"

#: ../panel-plugin/pulseaudio-menu.c:663
msgid "Choose Playlist"
msgstr "Zgjidhni Luajlistë"

